---
title: Page
metadata:
    helmet:
---
<!doctype html>
<html lang='en'>
    <head>
        <meta charset='UTF-8'>
        <meta name='viewport' content='width=device-width, initial-scale=1.0'>
        <title>Document</title>
        <link rel='stylesheet' href='{% getBundleFileUrl "css" 'tailwind' %}'>
        <link rel='stylesheet' href='{% getBundleFileUrl "css" %}'>
        {%- if metadata.helmet %}
            {{ helmet }}
        {%- endif %}
    </head>

    <body>
        {{ content }}
    </body>
</html>
{% css 'tailwind' %}
{% renderGlob 'assets/tailwind.css' %}
{% endcss %}
{% css %}
{% renderGlob 'components/**/*.css' %}
{% endcss %}
